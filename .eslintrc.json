{
    "env": {
        "es2021": true
    },
    "extends": [
        "airbnb-typescript/base",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "project": "tsconfig.json"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "rules": {
        "class-methods-use-this": "off"
//        "import/no-cycle": "off"
    }
}
